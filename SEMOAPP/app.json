{
  "expo": {
    "name": "SEMOAPP",
    "slug": "SEMOAPP",
    "version": "1.13.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app can access your photos if you are on Student Government for the purpose of updating the Student Government Feature Photo."
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.outpost.SEMOAPP",
      "buildNumber": "1.13.0",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses the location to show the user their location on the campus and shuttle maps.",
        "NSPhotoLibraryUsageDescription": "This app views the user's photo library after authentication to allow them to upload select images to the Student Government page."
      }

    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.zlaoch.SEMOAPP",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    }
  }
}
